{
  "name": "test262-to-mjsunit",
  "description": "Converts test262 tests to mjsunit tests",
  "keywords": [
    "test262",
    "mjsunit",
    "ecmascript",
    "testing"
  ],
  "version": "1.0.0-pre",
  "author": "Domenic Denicola <d@domenic.me> (https://domenic.me/)",
  "license": "WTFPL",
  "repository": "domenic/test262-to-mjsunit",
  "main": "lib/single-test.js",
  "bin": "bin/test262-to-mjsunit.js",
  "files": [
    "bin/",
    "lib/"
  ],
  "scripts": {
    "test": "baseline-tester . --cases=test/cases --input=test262.js --output=mjsunit.js",
    "lint": "jshint lib && jscs lib"
  },
  "dependencies": {
    "q": "^1.1.2",
    "recast": "^0.9.8",
    "test262-parser": "^2.0.1",
    "yargs": "^1.3.3"
  },
  "devDependencies": {
    "baseline-tester": "^1.0.0",
    "jscs": "^1.8.1",
    "jshint": "^2.5.10"
  }
}
